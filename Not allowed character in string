	for (int i = 0; i < name1.size(); ++i)
		{
			switch (name1[i])
			{
			case ';': case ':': case '"': case '\'' : 
			case '[' : case ']': case '*' : case '&' : 
			case '^' : case '%': case '$' : case '#': 
			case '@': case '!' :
					error("Not allowed symol occured in the name ; : \" \' [ ] * & ^ % $ # @ ! ");
			default:
				break;
			}
		}
